{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p watch:tailwind routify rollup",
    "ngdev": "run-p -n -l routify rollup ngrok watch:tailwind",
    "ngrok": "sleep 1 && ngrok http 5000 --log stdout",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic yarn run dev",
    "build": "yarn run build:tailwind && routify -b && rollup -c && routify export && workbox injectManifest",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export && cp dist/__app.html dist/404.html",
    "build:docker": "yarn run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && yarn run deploy",
    "deploy:netlify": "cd scripts/netlify && yarn run deploy",
    "rollup": "rollup -cw",
    "rollupProd": "rollup -c",
    "nollup": "nollup -c",
    "routify": "routify",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "nollup": "jakobrosenberg/nollup#custom-api-fallback",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.2",
    "rollup": "^2.18.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^6.1.0",
    "sirv": "^1.0.1",
    "spassr": "^1.1.1"
  },
  "dependencies": {
    "@material/button": "^7.0.0",
    "@material/menu": "^7.0.0",
    "@material/tab": "^7.0.0",
    "@material/tab-bar": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@sveltech/routify": "^1.9.1",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "ethers": "^5.0.2",
    "postcss-cli": "^7.1.1",
    "rollup-plugin-workbox": "^5.0.1",
    "svelte": "^3.23.2",
    "svelte-infinite-scroll": "^1.4.1",
    "tailwindcss": "^1.4.6",
    "workbox-cli": "^5.1.3"
  }
}
